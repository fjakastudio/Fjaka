.big-footer
    background: var(--background-footer)
    color: var(--background-footer-text)
    display: grid
    grid-template-columns: repeat(12, 1fr);
    grid-gap: 2.5rem;
    padding-top: 5.556vw
    border-radius: 4rem 4rem 0 0

.big-footer__center
    grid-column-start: 4;
    grid-column-end: 10;
    text-align: center;

.post-footer
    background: var(--background-post-footer)
