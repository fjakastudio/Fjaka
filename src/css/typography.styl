h1,
h2,
h3,
h4,
h5 
    font-family: p22-mackinac-pro, serif;
    font-weight: 800

h1,
.heading-one 
    font-size: clamp(2.563rem, 3.6vw + 1rem, 7.625rem)
    line-height: 1.1
    margin: 0

h2
    font-size: clamp(2rem, 3vw + 1.1rem, 5rem);
    margin: 0 0 1.5rem
    line-height: 1.1

.big-paragraph
    font-size: clamp(1rem, 0.75vw + 0.75rem, 2rem);
    line-height: 1.4
    margin: 0 0 1rem